<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite parallel="false" name="NopCommerce project">


    <listeners>
        <listener class-name="reportConfig.AllureTestListener"/>
    </listeners>


    <test name="Chrome Test" preserve-order="true">
        <parameter name="service" value="local"/>
        <parameter name="browser_name" value="Chrome"/>
        <parameter name="browser_version" value="115.0"/>
        <parameter name="ip_address" value="localhost"/>
        <parameter name="port" value="4445"/>

        <classes>
            <class name="user.Notebooks"/>
        </classes>
    </test>

    <test name="Firefox Test" preserve-order="true">
        <parameter name="service" value="local"/>
        <parameter name="browser_name" value="Firefox"/>
        <parameter name="browser_version" value="116.0"/>
        <parameter name="ip_address" value="localhost"/>
        <parameter name="port" value="4446"/>
        <classes>
            <class name="user.Notebooks"/>
        </classes>
    </test>

</suite>