<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite parallel="false" name="NopCommerce project">


    <listeners>
        <listener class-name="reportConfig.AllureTestListener"/>
    </listeners>


    <test name="Regression Test" preserve-order="true">
        <parameter name="service" value="local"/>
        <parameter name="os" value="Windows"/>
        <parameter name="os_version" value="10"/>
        <parameter name="browser_name" value="Chrome"/>
        <parameter name="browser_version" value="latest"/>
        <classes>
            <class name="commonTest.RegisterOnly"/>
            <class name="user.Login"/>
        </classes>
    </test>

</suite>
